{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "State",
  "type": "object",
  "required": [
    "max_size",
    "prng_seed",
    "score_count"
  ],
  "properties": {
    "max_size": {
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "prng_seed": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "score_count": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
