{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ScoreResponse",
  "type": "object",
  "required": [
    "description",
    "status"
  ],
  "properties": {
    "description": {
      "type": "string"
    },
    "score": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "status": {
      "type": "string"
    },
    "timestamp": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
